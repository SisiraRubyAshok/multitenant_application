  <script>
  

Highcharts.chart('monthly_chart', {

    chart: {
        height: (9 / 16 * 100) + '%' // 16:9 ratio
    },

    title: {
        text: 'Monthly Project Status'
    },

    subtitle: {
        text: ''
    },

    xAxis: {
        categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun',
            'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
    },

    series: [{
        data: [29.9, 71.5, 106.4, 129.2, 144.0, 176.0, 135.6, 148.5, 216.4, 194.1, 95.6, 54.4],
        type: 'column'
    }]

});

$(function () {
    $('#monthly_chart_dotted').highcharts({
       chart: {
        height: (9 / 16 * 100) + '%' // 16:9 ratio
    },
        xAxis: {
            categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
        },
        title: {
            text: 'Monthly Project Dotted Chart'  
        },
        series: [{
            data: [29.9, 71.5, 106.4, 129.2, 144.0, 176.0, 135.6, 148.5, 216.4, 194.1, 95.6, 54.4]        
        }]
    });
});
    $(function() {
        // Create the chart
        chart = new Highcharts.Chart({
            chart: {
                renderTo: 'chart-container1',
                type: 'pie'
            },
            
            plotOptions: {
                pie: {
                    shadow: true
                }
            },
            plotOptions: {
                    solidgauge: {
                        borderColor: '#009CE8',
                        borderWidth: 40,
                        radius: 90,
                        innerRadius: '90%',
                        dataLabels: {
                            y: 5,
                            borderWidth: 0,
                            format: '{y}%',
                            useHTML: true
                        }
                    }
                },
                title: {
                     text: '60',
                     align: 'center',
                     verticalAlign: 'middle',
                     color: 'black'
                    },
                pane: {
                    size: '100%',
                    center: ['50%', '60%'],
                    startAngle: -130,
                    endAngle: 130,
                    background: {
                    borderWidth: 20,
                    backgroundColor: '#DBDBDB',
                    shape: 'arc',
                    borderColor: '#DBDBDB',
                        outerRadius: '90%',
                        innerRadius: '90%'
                    }
                },
            colors: [
                '#00ff00'
            ],
            series: [{
                name: 'Active Projects',
                data: [["Total",60]],
                size: '100%',
                y:60,
                innerSize: '85%',
                format: '{y}%',
                showInLegend:false,
                dataLabels: {
                    enabled: false
                }
            }],
            
        });
    });
    
    $(function() {
        // Create the chart
        chart = new Highcharts.Chart({
            chart: {
                renderTo: 'chart-container2',
                type: 'pie'
            },
            title: {
                text: ''
            },
            plotOptions: {
                pie: {
                    shadow: true
                }
            },
            plotOptions: {
                    solidgauge: {
                        borderColor: '#009CE8',
                        borderWidth: 40,
                        radius: 90,
                        innerRadius: '90%',
                        dataLabels: {
                            y: 5,
                            borderWidth: 0,
                            useHTML: true
                        }
                    }
                },
                 title: {
                     text: '20',
                     align: 'center',
                     verticalAlign: 'middle',
                     color: 'black'
                    },
                pane: {
                    size: '100%',
                    center: ['50%', '60%'],
                    startAngle: -130,
                    endAngle: 130,
                    background: {
                    borderWidth: 20,
                    backgroundColor: '#DBDBDB',
                    shape: 'arc',
                    borderColor: '#DBDBDB',
                        outerRadius: '90%',
                        innerRadius: '90%'
                    }
                },
            colors: [
                'red'
            ],
            series: [{
                name: 'Inactive projects',
                data: [["Total",20]],
                size: '100%',
                innerSize: '85%',
                showInLegend:false,
                dataLabels: {
                    enabled: false
                }
            }]
        });
    });
    
    $(function() {
  $('#chart-container3').highcharts({
    chart: {
      plotBackgroundColor: null,
      plotBorderWidth: null,
      plotShadow: false,
      type: 'pie'
    },
    title: {
      text: ''
    },
    tooltip: {
      pointFormat: '{series.name}: <br>{point.percentage:.1f} %<br>total: {point.total}'
    },
    plotOptions: {
      pie: {
        dataLabels: {
          enabled: true,
          format: '<b>{point.name}</b>:<br>{point.percentage:.1f} %<br>total: {point.total}',
        }
      }
    },
    series: [{
      colorByPoint: true,
      data: [{
        name: 'Monthly',
        y: 15
      }, {
        name: 'Weekly',
        y: 30
      },
      {
        name: 'Quaterly',
        y: 30
      }]
    }]
  });
});
    $(document).ready(function(){
var height_value = $("#client_dash_board").height();
$('.side_navigation').css('height', height_value);
});


$(document).ready(function(){
$( "#close_navigation" ).click(function() {
$(".side_navigation").removeClass("expand_side_navigation");
$(".main_data_section_view").removeClass("minimize_data_section");
$("#close_navigation").hide();
$("#show_navigation").show();
$(".side_navigation").addClass("minmize_side_navigation");
$(".main_data_section_view").addClass("expand_data_section");
});
});

$(document).ready(function(){
$("#show_navigation").click(function() {
$("#close_navigation").show();
$("#show_navigation").hide();
$(".side_navigation").addClass("expand_side_navigation");
$(".main_data_section_view").addClass("minimize_data_section");
});
});



  </script>
